{
  "extends": "airbnb-base",
  "env": {
    "es6": true,
    "mocha": true,
    "node": true,
    "browser": true
  },
  "rules": {
    "linebreak-style": 0,
    "comma-dangle": 0,
    "no-console": 0,
    "import/prefer-default-export": 0,
    "no-param-reassign": 0,
    "import/no-named-as-default-member": 0,
    "import/named": 0,
    "import/no-named-as-default": 0,
    "no-unused-vars": 0
  }
}
